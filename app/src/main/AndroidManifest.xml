<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.atry">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ponder_logo_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ponder_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
            android:label="@string/app_name"></activity>
        <activity android:name=".ChapterActivity" />
        <activity android:name=".TestOptionsActivity" />
        <activity android:name=".TestStartActivity" />
        <activity android:name=".FeedbackActivity"></activity>
        <activity android:name=".ThankYouActivity"></activity>
    </application>

</manifest>